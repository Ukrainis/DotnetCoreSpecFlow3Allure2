{
  "allure": {
    "directory": "allure-results",
    "allure.enableParameters": true,
    "links": [
      "https://www.google.com/search?q={issue}",
      "https://www.google.com/search?q={tms}"
    ]
  },
  "specflow": {
    "links": {
      "link": "^link:(.+)",
      "issue": "^issue:(\\d+)",
      "tms": "^tms:(\\d+)"
    },
    "labels": {
      "owner": "^owner:?(.+)",
      "severity": "^(normal|blocker|critical|minor|trivial)"
    }
  }
}
